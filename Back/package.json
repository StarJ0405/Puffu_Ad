{
  "name": "my-express-ts-app",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src/**/*.ts --exec ts-node --files src/app.ts",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc && tsc-alias",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/data-source.ts",
    "migration:generate": "npm run typeorm:cli migration:generate",
    "migration:create": "npm run typeorm:cli migration:create",
    "migration:run": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm:cli migration:revert"
  },
  "dependencies": {
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.19",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.2",
    "@types/winston": "^2.4.4",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.4.0",
    "fs": "^0.0.1-security",
    "googleapis": "^159.0.0",
    "lodash": "^4.17.21",
    "lunisolar": "^2.6.0",
    "multer": "^2.0.1",
    "node-cron": "^4.2.0",
    "nodemailer": "^7.0.5",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.25",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.12",
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^3.0.2",
    "cross-env": "^7.0.3",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
